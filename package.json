    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "postcss": "^8",
